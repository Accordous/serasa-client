{
    "name": "accordous/serasa-client",
    "description": "Client for Serasa API",
    "type": "library",
    "authors": [
        {
            "name": "Pedro Augusto",
            "email": "pams.pedro@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Accordous\\SerasaClient\\Providers\\SerasaClientServiceProvider"
            ],
            "aliases": {
              "SerasaClient" : "Accordous\\SerasaClient\\Facades\\SerasaClient"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Accordous\\SerasaClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Accordous\\SerasaClient\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^7.0|^8.0",
        "illuminate/http": "^7.0|^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.3",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    }
}
